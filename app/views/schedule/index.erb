<table>
    <tr>
    <th>Time</th>
    <% @providers.each do |provider| %>
        <th><%= provider.full_name %></th>
    <% end %>
    </tr>
    <% 25.times do |i| %>
        <% if i>= 12 %> 
            <% i += 3 %>
        <% end %>
        <tr>
        <th><%= "#{i/3 + 8}:#{i%3 * 2}0" %></th>
        <% @providers.each do |provider| %>
            <td></td>
        <% end %>
        </tr>
    <% end %>
</table>